{
    "name": "nightpdf",
    "version": "0.4.9",
    "description": "Dark Mode for PDFs",
    "main": "out/app.js",
    "packageManager": "yarn@3.4.1",
    "repository": "https://github.com/Lunarequest/NightPDF",
    "homepage": "https://github.com/Lunarequest/NightPDF#readme",
    "bugs": {
        "url": "https://github.com/Lunarequest/NightPDF/issues",
        "email": "nullrequest@vivaldi.net"
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "rimraf out && tsc && rollup -c rollup.config.mjs",
        "pack": "electron-builder --dir",
        "dev": "yarn build && electron .",
        "dist": "yarn build && electron-builder",
        "dist-win": "yarn build && electron-builder --win",
        "lint": "eslint",
        "prettier": "prettier --config .prettierrc --write ."
    },
    "author": {
        "name": "Nullrequest",
        "email": "nullrequest@vivaldi.net"
    },
    "license": "GPL-2.0-only",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "electron": "^23.0.0",
        "electron-builder": "^23.6.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.4",
        "rimraf": "^4.1.2",
        "rollup": "^3.15.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "electron-updater": "^5.3.0",
        "nouislider": "^15.7.0"
    }
}
