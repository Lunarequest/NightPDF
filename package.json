{
	"name": "nightpdf",
	"version": "2.0.3",
	"description": "Dark Mode for PDFs",
	"main": "out/main/app.js",
	"packageManager": "yarn@4.0.0-rc.48",
	"repository": "https://github.com/Lunarequest/NightPDF",
	"homepage": "https://github.com/Lunarequest/NightPDF#readme",
	"bugs": {
		"url": "https://github.com/Lunarequest/NightPDF/issues",
		"email": "nullrequest@vivaldi.net"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "rimraf out && gulp",
		"dev": "yarn build && DEBUG=1 electron .",
		"dist": "NODE_ENV='production' NODE_OPTIONS='--max-old-space-size=4096' yarn build && NODE_ENV='production' NODE_OPTIONS='--max-old-space-size=4096' electron-builder --config electron-builder-config.js",
		"lint": "rome check .",
		"format": "rome format . --write",
		"ci": "rome ci .",
		"pre-commit": "yarn ci",
		"postinstall": "husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable"
	},
	"author": {
		"name": "Lunarequest",
		"email": "nullrequest@vivaldi.net"
	},
	"license": "GPL-2.0-only",
	"devDependencies": {
		"@types/gulp": "^4.0.13",
		"@types/gulp-postcss": "^8.0.3",
		"@types/gulp-sass": "^5.0.1",
		"@types/node": "^20.3.3",
		"@types/yargs": "^17.0.24",
		"cssnano": "^6.0.1",
		"cssnano-preset-advanced": "^6.0.1",
		"electron": "^25.2.0",
		"electron-builder": "^24.5.2",
		"esbuild": "^0.18.11",
		"gulp": "^4.0.2",
		"gulp-esbuild": "^0.11.2",
		"gulp-postcss": "^9.0.1",
		"gulp-sass": "^5.1.0",
		"husky": "^8.0.3",
		"pinst": "^3.0.0",
		"postcss": "^8.4.24",
		"rimraf": "^5.0.1",
		"rome": "^12.1.3",
		"sass": "^1.63.6",
		"ts-node": "^10.9.1"
	},
	"dependencies": {
		"electron-log": "^4.4.8",
		"electron-store": "^8.1.0",
		"electron-tabs": "^1.0.1",
		"electron-updater": "6.1.3",
		"nouislider": "^15.7.1",
		"typescript": "^5.1.6",
		"yargs": "^17.7.2"
	}
}
